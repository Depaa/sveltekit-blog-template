{
	"name": "frontend",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 5173 --open --mode quality",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"deploy:quality": "vite build --mode quality",
		"deploy:production": "vite build --mode production"
	},
	"dependencies": {
		"aws-amplify": "^5.3.11",
		"dompurify": "^3.0.6",
		"easymde": "^2.18.0",
		"eslint-plugin-svelte": "^2.34.1",
		"font-awesome": "^4.7.0",
		"highlight.js": "^11.9.0",
		"object-hash": "^3.0.0",
		"sass": "^1.69.5",
		"svelte-inview": "^4.0.1"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^5.0.8",
		"@mdepascale/sveltekit-aws-adapter": "^0.0.1",
		"@neoconfetti/svelte": "^1.0.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.27.2",
		"@types/cookie": "^0.5.3",
		"@types/dompurify": "^3.0.4",
		"@types/node": "^20.8.10",
		"@types/object-hash": "^3.0.5",
		"@types/turndown": "^5.0.3",
		"@typescript-eslint/eslint-plugin": "^6.9.1",
		"@typescript-eslint/parser": "^6.9.1",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^9.0.0",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2",
		"ts-node": "^10.9.1",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.5.0"
	}
}
